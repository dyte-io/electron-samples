{
  "extends": "./paths.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "Node16",
    "resolveJsonModule": true,
    "strict": true,
    "jsx": "react-jsx"
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
      "./dist/**/*",
      "./release/**/*",
      "./scripts/**/*",
      "./node_modules/**/*",
  ]
}